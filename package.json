{
  "name": "yak-a11y",
  "version": "1.0.0",
  "description": "Comprehensive accessibility checker with detailed explanations and documentation links",
  "keywords": [
    "accessibility",
    "a11y",
    "wcag",
    "axe-core",
    "testing"
  ],
  "homepage": "https://www.npmjs.com/package/yak-a11y",
  "repository": {
    "type": "git",
    "url": "https://github.com/yakbrother/yak-a11y.git"
  },
  "license": "MIT",
  "author": "yakbrother",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "yak-a11y": "dist/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "FORCE_COLOR=1 vitest run",
    "test:watch": "FORCE_COLOR=1 vitest watch",
    "test:coverage": "FORCE_COLOR=1 vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "examples": "node examples/server.js",
    "check": "yak-a11y"
  },
  "dependencies": {
    "@axe-core/puppeteer": "^4.10.1",
    "axe-core": "^4.10.3",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "jsdom": "^26.0.0",
    "puppeteer": "^24.4.0",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@types/axe-core": "^2.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@types/puppeteer": "^5.4.7",
    "@vitest/coverage-v8": "^3.0.8",
    "happy-dom": "^15.10.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  }
}
